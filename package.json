{
  "name": "@kylemit/stack-posts",
  "version": "1.0.0",
  "description": " An offline, searchable, personalized copy of Stack Overflow",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "full-build": "npm run clean && npm run build",

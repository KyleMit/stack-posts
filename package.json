    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "17.0.37",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
